</nav>
<div class="container">
  <div class="form-group row">
      <label for="select" class="col-xs-1 control-label">Filter:</label>
      <div class="col-xs-3">
        <select class="form-control" id="select">
          <option>All</option>
          <option>Club Positions</option>
          <option>Freelance Tasks</option>
        </select>
      </div>
      <div class="col-xs-3">
        <select class="form-control" id="select">
          <option>All</option>
          <option>Coding/Web-design</option>
          <option>Event Planning</option>
          <option>Funding/Treasury</option>
          <option>...</option>
        </select>
      </div>
      <div class="col-xs-3">
        <select class="form-control" id="select">
          <option>All</option>
          <option>Club Type</option>
        </select>
      </div>
      <div class="col-xs-2">
      <button type="submit" class="btn btn-primary" style="background-color: #00274c; color: #ffcb05;">GO</button>
      </div>
    </div>
    <hr>
<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>ID #</th>
      <th>Position:</th>
      <th>Date Posted:</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  	<% for(var post=0; post<postinglist.length; post++) { %>
	    <tr>
	      <td><h5>1</h5></td>
	      <td><h4>
	      <a href="#/" style="color: #00274c; text-decoration: underline;">
	      <%= postinglist[post].title %></a>
	 	  </h4><%= postinglist[post].club %><br>Applicants Received: 16</td>
	      <td><%= postinglist[post].createdAt %><br><%= postinglist[post].expireAt %></td>
	      <td><button type="submit" class="btn btn-block btn-primary" style="background-color: #00274c; color: #ffcb05;">Apply</button></td>
	    </tr>
    <% } %>
  </tbody>
</table>
  <hr>
  <div style="text-align: center;">
      <ul class="pagination">
        <li class="disabled"><a href="#"><<</a></li>
        <li class="active"><a href="#">1</a></li>
